
# ============================
# .env.example — Nawah Core
# ملف مثال يوضح المتغيرات المطلوبة للتشغيل محليًا
# لا يحتوي على بيانات حساسة
# ============================

# شبكة البلوكشين
BSC_MAINNET_RPC_URL=https://bsc-dataseed.binance.org/
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545

# مفاتيح خاصة (اتركها فارغة للمطورين لملئها بأنفسهم)
PRIVATE_KEY=

# بيانات التوكن
TOKEN_NAME=Nawah Token
TOKEN_SYMBOL=NWTK
TOKEN_TOTAL_SUPPLY=100000000

# إعدادات قاعدة البيانات
DB_HOST=localhost
DB_PORT=5432
DB_USER=nawah_user
DB_PASSWORD=
DB_NAME=nawah_core_db

# API Keys لمزودي الخدمات الخارجية (اتركها فارغة)
INFURA_API_KEY=
ALCHEMY_API_KEY=
ETHERSCAN_API_KEY=

# إعدادات الخادم
PORT=4000
NODE_ENV=development
